{
  "name": "@ims/ui",
  "version": "2.0.0",
  "description": "供应链订货系统共享UI组件库 (基于 shadcn/ui)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
